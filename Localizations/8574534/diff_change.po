msgstr "Корабельные жетоны"

msgid "IDS_SYS_MSG_BOUGHT_EVENTUM_9"
msgstr "Корабельные жетоны куплены"

msgid "IDS_SYS_MSG_RECEIVED_EVENTUM_9"
msgstr "Получены корабельные жетоны"

msgid "IDS_SYS_MSG_WRITTEN_OFF_EVENTUM_9"
msgstr "Списаны корабельные жетоны"
